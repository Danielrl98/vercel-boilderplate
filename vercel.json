{
  "version": 2,
  "builds": [
    {
      "src": "dist/**",
      "use": "@vercel/node"
    },
    {
      "src": "dist/**",
      "use": "@vercel/static",
      "config": { "includeFiles": ["dist/**"] }
    }
  ],
  "routes": [
    {
      "src": "/dist/(.*)",
      "dest": "dist/index.js"
    },
    {
      "src": "/(.+)",
      "dest": "dist/$1"
    }
  ]
}
